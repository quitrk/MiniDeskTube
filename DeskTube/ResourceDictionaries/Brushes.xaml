<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="LightBlueBrush" Color="#0097fb"/>
    <SolidColorBrush x:Key="DarkBlueBrush" Color="#204d6d"/>
    
    <SolidColorBrush x:Key="ForegroundBrush" Color="#cccccc"/>
    <SolidColorBrush x:Key="BackgroundBrush" Color="#2d2d30"/>
    <SolidColorBrush x:Key="SecondaryBackgroundBrush" Color="#252526"/>
    
    <SolidColorBrush x:Key="BorderBrush" Color="#555555"/>
    
    <SolidColorBrush x:Key="TransparentBackgroundBrush" Color="#00000000"/>

    <SolidColorBrush x:Key="BaseBackgroundBrush" Color="Black"/>
    <SolidColorBrush x:Key="BaseForegroundBrush" Color="White"/>
</ResourceDictionary>